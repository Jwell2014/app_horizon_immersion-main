{% extends 'base_ju.html.twig' %}

{% block title %}Fichier{% endblock %}

{% block body %}
    <h1>Fichier</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fichier.id }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ fichier.image }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ fichier.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_fichier_index') }}">back to list</a>

    <a href="{{ path('app_fichier_edit', {'id': fichier.id}) }}">edit</a>

    {{ include('admin_interface/fichier/_delete_form.html.twig') }}
{% endblock %}
